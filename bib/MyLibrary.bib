@article{kouhestaniIFCbasedProcessMining2020,
  title = {{{IFC-based}} Process Mining for Design Authoring},
  author = {Kouhestani, Sobhan and Nik-Bakht, Mazdak},
  date = {2020-04-01},
  journaltitle = {Automation in Construction},
  volume = {112},
